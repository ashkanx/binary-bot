{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env": {
    "es6": true,
    "jasmine": true
  },
  "rules": {
    "indent": [0, 4],
    "id-length": [2, { min: 1 }],
    "comma-dangle": [2, "always-multiline"],
    "no-eq-null": 2,
    "block-scoped-var": 2,
    "no-nested-ternary": 2,
    "arrow-body-style": 0
  }
}
